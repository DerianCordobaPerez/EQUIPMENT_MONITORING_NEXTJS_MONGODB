{
  "commands": {
    "memory": "free",
    "disk": "df -h",
    "ip": "ifconfig",
    "ports": "netstat -lu && netstat -lt",
    "users": "lastb",
    "process": "ps -aux | tail --lines=10",
    "table": "netstat -nr",
    "client": "hostname",
    "bandwidth": "cat /proc/net/dev"
  },
  "roles": {
    "dhcp": "dhcp",
    "dns": "dns",
    "web": "web",
    "snmp": "snmp",
    "client": "client"
  },
  "services": {
    "dhcp": "isc-dhcp-server",
    "bind": "bind9",
    "snmp": "snmpd",
    "apache2": "apache2"
  }
}
